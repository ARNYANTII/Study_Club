package ISC;
import java.util.Scanner;
public class day30 {
    public static void main(String[] args) {
    Scanner sc = new Scanner (System.in);
        System.out.print("Masukkan angka : ");
        int angka = sc.nextInt();
        
        for (int i = 0; i <= angka; i++) {
            if (i % 3 !=  0 && i % 2 == 0) {
                System.out.println(i);
            }
        }
    }
}
